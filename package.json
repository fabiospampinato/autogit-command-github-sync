{
  "name": "autogit-command-github-sync",
  "description": "A command for synchronizing repositories with GitHub.",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck",
    "compile:watch": "tsc --skipLibCheck --watch",
    "lint": "tslint \"src/**/*.ts*\" --force",
    "prepublish": "npm run clean && npm run lint && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/autogit-command-github-sync/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/autogit-command-github-sync.git"
  },
  "keywords": [
    "autogit",
    "command",
    "github",
    "sync"
  ],
  "dependencies": {
    "autogit-plugin-github-sync-description": "^1.0.0",
    "autogit-plugin-github-sync-keywords": "^1.0.0",
    "autogit-plugin-pull": "^1.0.0",
    "autogit-plugin-push": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^10.9.4",
    "rimraf": "^2.6.0",
    "tslint": "^4.4.2",
    "typescript": "^3.0.3"
  }
}
